@model ConsoleApplication.Models.Student

<div class="form-group">          
    <label asp-for="FirstName" class="col-md-2 control-label"></label>
    <div class="col-md-4">
        <input class="form-control "  asp-for="FirstName">
    </div>
</div>
    
<div class="form-group">       
    <label asp-for="LastName" class="col-md-2 control-label"></label>
    <div class="col-md-4">
        <input class="form-control " asp-for="LastName" >
    </div>
</div>

<div class="form-group">       
    <label asp-for="EnrollmenDate" class="col-md-2 control-label"></label>
    <div class="col-md-4">
        <input class="form-control " asp-for="EnrollmenDate" >
    </div>
</div>

<div class="form-group">       
    <label asp-for="Age " class="col-md-2 control-label"></label>
    <div class="col-md-4">
        <input class="form-control " asp-for="Age">
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset-2 col-md-4">
        <button type="submit" class="btn btn-success">@ViewBag.btnText</button>
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset-3 col-md-4">
        <button type="button" class="btn btn-primary">@ViewBag.btn2Text</button>
    </div>
</div>